# Backend API URL
REACT_APP_API_URL=http://localhost:8080/api

# AI Service URL (Python)
REACT_APP_AI_URL=http://localhost:5000/api

# OpenWeatherMap API Key (đăng ký tại: https://openweathermap.org/api)
REACT_APP_WEATHER_API_KEY=your_api_key_here

# Google Maps API Key (nếu dùng Google Maps thay vì Leaflet)
REACT_APP_GOOGLE_MAPS_KEY=your_google_maps_key_here

# OpenAI API Key (nếu dùng ChatGPT)
REACT_APP_OPENAI_KEY=your_openai_key_here

# AWS Region (nếu dùng AWS Bedrock)
REACT_APP_AWS_REGION=ap-southeast-1

# Environment
REACT_APP_ENV=development
